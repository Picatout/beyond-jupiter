  63 1a98 40F8042B 	1:  str T2,[T0],#4
  64 1a9c 0139     	    subs T1,#1
  65 1a9e FBD1     	    bne 1b 
  66 1aa0 2262     	    str T2,[UP,#TMP] // clear tmp variable (SPARE)
  67              	
